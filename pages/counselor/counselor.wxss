/* pages/counselor/counselor.wxss */
.table{
  border-style: solid;
  border-width: 4rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-color: #8A8A8A;
}

.tr{
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}

.td{
  width: 40%;
  justify-content: center;
  text-align: center; 
}

.bg-header{
  background: #FFE4B5;
}

.bg-items{
  background:#E6F3F9;
}

.th{
  width: 40%;
  color: #080808;
  display: inline-block;
  text-align: center;
}